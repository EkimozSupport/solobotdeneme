{
  "name": "Mamaklimusic",
  "description": "Telegram bot + userbot for streaming audio in group calls.",
  "keywords": ["telegram", "python"],
  "repository": "https://github.com/mamakli06/m-sicbot",
  "stack": "container",
  "env": {
    "STRING_SESSION": {
      "description": "A GramJS/Telethon string session. You can generate one at https://rojserbest.github.io/bssg.",
      "required": true
    },
    "BOT_TOKEN": {
      "description": "A bot token from @BotFather.",
      "required": true
    },
    "API_ID": {
      "description": "An app ID from https://my.telegram.org/apps.",
      "required": true
    },
    "API_HASH": {
      "description": "An app hash from https://my.telegram.org/apps.",
      "required": true
    }
  }
}
